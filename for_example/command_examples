## bonus ##
aa && ls
ls | aa && ls
aa && ls && ls
aa && ls && ls | ls
aa && ls && ls | pwd ; ls
ls && aa && ls

ls || pwd && echo a
false && true || echo 'Error'
true || false && echo 'Yes'
true || echo aaa && echo bbb


